{
  "name": "SOC Automation Environment",
  "dockerComposeFile": "docker-compose.yml",
  "service": "soc",
  "workspaceFolder": "/workspace",
  "forwardPorts": [9000, 9200, 5601],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "elastic.vscode-elasticsearch",
        "redhat.vscode-yaml"
      ]
    }
  },
  "postCreateCommand": "pip install -r requirements.txt"
}
